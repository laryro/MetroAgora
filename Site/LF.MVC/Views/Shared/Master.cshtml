@model BaseModel
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Kleenex</title>

    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <link href="~/Assets/Images/common/logo.ico" type="image/x-icon" rel="shortcut icon">

    <link rel="stylesheet" href="~/Assets/Components/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Assets/Styles/common/common.css" />
    @RenderSection("Style", required: false)

    <script type="text/javascript" src="~/Assets/Components/jquery/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="~/Assets/Components/jquery-migrate/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="~/Assets/Components/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Assets/Components/maskedinput/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="~/Assets/Scripts/common/common.js"></script>
    @RenderSection("Script", required: false)

</head>

<body>
    <header>
        @Html.Partial("Header")
    </header>
    <main>
        <div class="container">
            @RenderBody()
        </div>
        <div class="push"></div>
    </main>
    <footer>
        @Html.Partial("Footer")
    </footer>
</body>
</html>